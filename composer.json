{
    "name": "voltra/whitebox",
    "description": "A lightweight PHP micro framework to create dynamic websites easily",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Voltra",
            "email": "ludwig.guerin.98@gmx.fr"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1.11"
        }
    },
    "autoload": {
        "psr-4": {
            "WhiteBox\\" : "src"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "myclabs/php-enum": "^1.5",
        "http-interop/response-sender": "^1.0",
        "doctrine/annotations": "^1.6",
        "php-di/php-di": "^5.4",
        "hassankhan/config": "^0.11.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "php-mock/php-mock": "^2.0"
    }
}
